<Window x:Class="PeminSpectrumAnalyser.SettingsWindow_New"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PeminSpectrumAnalyser"
        mc:Ignorable="d"
        Title="SettingsWindow_New" Height="700" Width="900">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1.2*"/>  <!--Заголовок Подключение-->
            <RowDefinition Height="2*"/>
            <RowDefinition Height="1.2*"/>  <!--Заголовок Папка хранения экспериментов-->
            <RowDefinition Height="*"/>
            <RowDefinition Height="1.2*"/>  <!--Заголовок Настройки ИП-->
            <RowDefinition Height="4*"/>
            <RowDefinition Height="*"/>     <!--Заголовок Экран ИП-->
            <RowDefinition Height="9*"/>
        </Grid.RowDefinitions>
    </Grid>
</Window>
